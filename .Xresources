Xft.dpi: 175

! urxvt settings
URxvt*transparent: false
URxvt*depth: 32
URxvt*pointerShape: arrow
URxvt*background: [70]#383C4A
URxvt*foreground: white
URxvt*font: xft:Hermit:size=13
URxvt*boldFont: xft:Hermit:size=13
URxvt*renderFont: true
URxvt*allowBoldFonts: false
URxvt*scrollBar: false
URxvt*scrollstyle: rxvt

!!Theme Nine
URxvt.color0  :    #000000
URxvt.color1  :    #9e1828
URxvt.color2  :    #cf9ebe
URxvt.color3  :    #968a38
URxvt.color4  :    #414171
URxvt.color5  :    #963c59
URxvt.color6  :    #418179
URxvt.color7  :    #bebebe
URxvt.color8  :    #666666
URxvt.color9  :    #cf6171
URxvt.color10 :    #7cbc8c
URxvt.color11 :    #fff796
URxvt.color12 :    #4186be
URxvt.color13 :    #cf9ebe
URxvt.color14 :    #71bebe
URxvt.color15 :    #ffffff

! xterm settings
XTerm.termName: xterm-256color

xterm*pointerShape: arrow
xterm*background: black
xterm*foreground: white
xterm*faceName: Hermit
xterm*faceSize: 12
xterm*renderFont: true
xterm*allowBoldFonts: false


__scratchpad*pointerShape: arrow
__scratchpad*background: black
__scratchpad*foreground: white
__scratchpad*faceName: Hermit
__scratchpad*faceSize: 9
__scratchpad*renderFont: true
__scratchpad*allowBoldFonts: false

!Theme Nine
